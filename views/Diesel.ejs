<% include ./static/sidebar %>
  <div class="container">
    <container class="cuerpo">
      <form action="/addDiesel"  method="post">
        <br><br><br>
      
          
          
      
      <br>
      <label for="semana">Period:</label><br>
      <input type="number" id="week" name="week" placeholder="week"  required><br><br>
      
        <input type="text" id="Username" name="username" placeholder="" value="<%=user.username;%>" hidden><br><br>
        <label for="Invoice">Invoice:</label><br>
        <input type="text" id="Invoice" name="Invoice" placeholder="Invoice" value="" required><br><br>
        <label for="Fecha">Date:</label><br>
        <input type="date" id="Fecha" name="Date" placeholder="Date" value="" require><br><br>
        <label for="Tienda">Store:</label><br>
        <input type="text" id="Store" name="Store" placeholder="Store"  required><br><br>
        <label for="Millas">Miles:</label><br>
        <input type="number" step="0.001" id="Miles" name="Miles" placeholder="Miles"  required><br><br>
        <label for="Galones">Gallons:</label><br>
        <input type="number" step="0.001"id="Gallons" name="Gallons" placeholder="Gallons"  required><br><br>
        <label for="Costo">Amount:</label><br>
        <input type="number" step="0.001" id="Amount" name="Amount" placeholder="Amount"  required><br><br>
      

       
        <button type="submit" class="btn btn-primary">
          Confirmar
        </button>
      </form>
    </container>
    <div class="container">
      <div class="row mt-5">
          <div class="col-md-12">
              <br><br>
              <!-- <form action="/buscar" method="post"> <input type="number" placeholder="Semana" id="txtBuscar"
                      name="txtBuscar">
                  <button type="submit">Buscar</button> -->

              </form>
      
              <table class="table table-bordered table-hover">
                  <thead>
                      <tr>
                          <!-- <th>id</th> -->
                          <th># </th>
                          <th>Week</th>
                          <th>username</th>
                          <th>Invoice</th>
                          <th>Date</th>
                          <th>Store</th>
                          <th>Miles</th>
                          <th>Gallons</th>
                          <th>Amount</th>
                          

                      </tr>
                  </thead>
                  <tbody>

                      <%if (data1){%>
                          <%=console.log(data1.length);%>

                              <%for(var i=0;i < data1.length;i++){ %>

                                  <%let fechaMoment=(moment(data1[i].date)); const dformat='YYYY-MM-DD' ; let
                                      fechamiformato=fechaMoment.format(dformat);%>

                                      <tr>
                                    
                                          <td>
                                            <%=(i+1)%>
                                        </td>
                                          <td>
                                              <%= data1[i].week%>
                                          </td>
                                          <td>
                                              <%= data1[i].username%>
                                          </td>
                                          <td>
                                            <%= data1[i].Invoice%>
                                        </td>
                                          <td>
                                              <%= fechamiformato%>
                                          </td>
                                         
                                          <td>
                                              <%= data1[i].Store%>
                                          </td>
                                          <td>
                                              <%= data1[i].Miles%>
                                          </td>
                                          <td>
                                              <%= data1[i].Gallons%>
                                          </td>
                                          <td>
                                              <%= data1[i].Amount%>
                                          </td>

                                          
                                        


                                      </tr>
                                      <%}%>
                                          <%}%>

                  </tbody>
              </table>

          </div>
