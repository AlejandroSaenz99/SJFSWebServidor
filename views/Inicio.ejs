<% include ./static/sidebar %>
  <div class="container">
    <container class="cuerpo">
      <form action="/add"  method="post">
        <br><br><br>
        <h5>"Buen Inicio de Semana Vamos por esos 12 viajes!"</h5>
        <style>
         h5{
font-family: cursive;
color:cornflowerblue;
text-shadow: 5px 5px 20px black;

         }
          
          
      </style>
      <br>
        <input type="text" id="Username" name="username" placeholder="" value="<%=user.username;%>" hidden><br><br>
        <label for="semana">Week:</label><br>
        <input type="number" id="week" name="week" placeholder="week"  required><br><br>
        <label for="semana">Date:</label><br>
        <input type="date" id="Fecha" name="date" placeholder="Date" value="" require><br><br>
        <label for="semana">BOL:</label><br>
        <input type="text" id="BOL" name="bol" placeholder="BOL" value="" required><br><br>
        <label for="semana">Weigth:</label><br>
        <input type="number" step="0.001" id="Peso" name="weight" placeholder="Weigth" value="" required><br><br>
        <label for="tipo">Ton/Pound:</label><br>
        <select name="type">
          <option value="Ton">Ton</option>
          <option value="Pound">Pound</option>

        </select>
        <br><br>
        <label for="semana">Miles:</label><br>
        <input type="number"step="0.001" id="Distancia" name="miles" placeholder="Miles" value="" required><br><br>
        <label for="semana">Load Facility:</label><br>
        <input type="text" id="Destino" name="load_facility" placeholder="Load Facility" value="" required><br><br>
        <label for="Nonta">Nota:</label><br>
        <input type="text" id="Nota" name="Nota" placeholder="Nota" value="" ><br><br>
        
        <button type="submit" class="btn btn-primary">
          Registrar Viaje
        </button>
      </form>
    </container>
    <div class="container">
      <div class="row mt-5">
          <div class="col-md-12">
              <br><br>
              <!-- <form action="/buscar" method="post"> <input type="number" placeholder="Semana" id="txtBuscar"
                      name="txtBuscar">
                  <button type="submit">Buscar</button> -->

              </form>
      
              <table class="table table-bordered table-hover">
                  <thead>
                      <tr>
                          <!-- <th>id</th> -->
                          <th># Trip</th>
                          <th>Week</th>
                          <th>username</th>
                          <th>date</th>
                          <th>bol</th>
                          <th>weight</th>
                          <th>type</th>
                          <th>miles</th>
                          <th>load facility</th>
                          <th>Nota</th>

                        

                      </tr>
                  </thead>
                  <tbody>

                      <%if (data){%>
                          <%=console.log(data.length);%>

                              <%for(var i=0;i < data.length;i++){ %>

                                  <%let fechaMoment=(moment(data[i].date)); const dformat='YYYY-MM-DD' ; let
                                      fechamiformato=fechaMoment.format(dformat);%>

                                      <tr>
                                          <!-- <td> <%= data[i].Id%></td> -->
                                          <td>
                                            <%=(i+1)%>
                                        </td>
                                          <td>
                                              <%= data[i].week%>
                                          </td>
                                          <td>
                                              <%= data[i].username%>
                                          </td>
                                          <td>
                                              <%= fechamiformato%>
                                          </td>
                                          <td>
                                              <%= data[i].bol%>
                                          </td>
                                          <td>
                                              <%= data[i].weight%>
                                          </td>
                                          <td>
                                              <%= data[i].type%>
                                          </td>
                                          <td>
                                              <%= data[i].miles%>
                                          </td>
                                          <td>
                                            <%= data[i].load_facility%>
                                        </td>
                                        <td>
                                          <%= data[i].Nota%>
                                      </td>

                                          
                                        


                                      </tr>
                                      <%}%>
                                          <%}%>

                  </tbody>
              </table>

          </div>
        </div>
      </div>
    
</div>

